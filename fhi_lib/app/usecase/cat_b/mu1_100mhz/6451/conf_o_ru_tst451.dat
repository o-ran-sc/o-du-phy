antSrsC121=./usecase/cat_b/mu1_100mhz/ant_srs_57.bin
antSrsC122=./usecase/cat_b/mu1_100mhz/ant_srs_58.bin
antSrsC123=./usecase/cat_b/mu1_100mhz/ant_srs_59.bin
antSrsC124=./usecase/cat_b/mu1_100mhz/ant_srs_60.bin
antSrsC125=./usecase/cat_b/mu1_100mhz/ant_srs_61.bin
